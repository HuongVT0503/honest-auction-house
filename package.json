{
  "name": "honest-auction-house",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
    "build": "cd server && npm install && npm run build:full",
    "start": "cd server && npx prisma migrate deploy && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "dependencies": {
    "circomlib": "^2.0.5"
  }
}
